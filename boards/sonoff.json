{
  "build": {
    "core": "esp8266",
    "extra_flags": "-DESP8266 -DARDUINO_ARCH_ESP8266",
    "f_cpu": "80000000L",
    "f_flash": "40000000L",
    "flash_mode": "qio",
    "ldscript": "eagle.flash.1m64.ld",
    "mcu": "esp8266",
    "variant": "generic"
  },
  "connectivity": [
    "wifi"
  ],
  "frameworks": [
    "arduino",
    "simba",
    "esp8266-rtos-sdk",
    "esp8266-nonos-sdk"
  ],
  "name": "Sonoff",
  "upload": {
    "maximum_ram_size": 81920,
    "maximum_size": 1048576,
    "require_upload_port": true,
    "resetmethod": "none",
    "speed": 115200
  },
  "url": "https://www.itead.cc/sonoff-wifi-wireless-switch.html",
  "vendor": "ITEAD Intelligent Systems Co.Ltd."
}
